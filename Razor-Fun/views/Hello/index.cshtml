
<body>
    <h1>Here are some delicous foods!</h1>
@{
    var StringList = new List<string>
    {
        "Apple Pie",
        "Burritos",
        "Clams Casino",
        "Donuts"
    };
        
    foreach(var word in StringList)
    {
        char first = word[0];
        if(first == 'C')
        {
        <div>
            <p style="color: red">@word</p>
       </div>
        }
        else
        {
        <div>
            <p>@word</p>
        </div>   
        }

    }
}
</body>